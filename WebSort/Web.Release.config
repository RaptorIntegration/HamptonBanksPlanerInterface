        <add assembly="DevExpress.XtraCharts.v10.2.Web, Version=10.2.3.0, Culture=neutral, PublicKeyToken=B88D1754D700E49A" />
        <add assembly="DevExpress.XtraCharts.v10.2, Version=10.2.3.0, Culture=neutral, PublicKeyToken=B88D1754D700E49A" />
